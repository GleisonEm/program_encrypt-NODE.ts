    "dotenv": "^16.0.0",
    "express": "^4.17.2",
    "google-translate": "^3.0.0",
    "mongodb": "^4.4.0",
    "nodemon": "^2.0.15",
    "reflect-metadata": "^0.1.13",
    "socket.io": "^4.4.1",
    "typeorm": "^0.2.44",
    "uuid": "^8.3.2"
  },
